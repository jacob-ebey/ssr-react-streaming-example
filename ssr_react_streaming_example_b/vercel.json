{
  "redirects": [{ "source": "/_static/:match*", "destination": "/:match*" }],
  "rewrites": [
    { "source": "/", "destination": "/api/ssr?location=/" },
    { "source": "/b", "destination": "/api/ssr?location=/b" },
    { "source": "/b:match*", "destination": "/api/ssr?location=/b:match*" }
  ]
}
